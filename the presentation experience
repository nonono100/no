------------------- collect all codes -------------------
local codes = {
    "bookworm";
    "nootnoot";
    "itsaboutdriveitsaboutpower";
    "5gems";
    "emotionaldamage";
    "uwu";
    "minimalgamespro";
    "pencil";
    "megaboost";
    "hallway";
    "toilet";
    "azureoptix";
    "helicopter";
    "rat";
    "code";
    "teachermadcuzbad";
    "nikkocoder";
    "poop";
    "therearenootherteachersintheschoolbecausenobodywantstoseethebadteacher";
    "10points";
    "fartyreward";
    "flood";
    "funnybackrooms";
    "christmas2022";
    "christmasgift";
    "manfacepooper";
    "maxwellgood";
    }
for i,v in pairs(codes) do 
game:GetService("ReplicatedStorage").RedeemCode:InvokeServer(v)
wait(0.5)
    end

------------------- unlock hidden button -------------------

for i,v in pairs(game:GetService("Players").LocalPlayer.PlayerGui.SomeEmotes.actions.Lists:GetDescendants()) do 
    if v:IsA("TextButton") and v.Visible == false then
        v.Visible = true
        end
    end


------------------- Farm gems (autoexecute) -------------------

spawn(function()
    wait(20)
    game:Shutdown()
end)

local a = game:GetService("ReplicatedStorage"):WaitForChild("RedeemCode")
a:InvokeServer("uwu");

wait(0.5)--wait(0.3)

local Servers = game.HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/7772810845/servers/Public?sortOrder=Asc&limit=100"))
local servers_data = Servers.data
local random_index = math.random(1, #servers_data)
local random_server = servers_data[random_index]

game:GetService('TeleportService'):TeleportToPlaceInstance(game.PlaceId, random_server.id)

------------------- Farm points (autoexecute) -------------------


repeat wait() until game.Players and game.Players.LocalPlayer
if game.Players.LocalPlayer.Name ~= "" then
	if game.PlaceId == 9372535855 then
		spawn(function()
			wait(50)
			game:Shutdown()
		end)
		repeat wait() until game:IsLoaded()
		repeat wait() until game.Players.LocalPlayer.Character
		wait(1)
		for i, v in pairs(workspace:GetChildren()) do
			if v.Name == "egg" and v:FindFirstChild("TouchInterest") and game.Players.LocalPlayer.Character:FindFirstChild("Head") then
				firetouchinterest(v, game.Players.LocalPlayer.Character.Head, 0)
			end
		end
		getrenv().shared.skip = true
		if game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = workspace["Busy man"].Head.CFrame
		end
		game:GetService("TeleportService"):Teleport(7841304988, game.Players.LocalPlayer);
	end
	if game.PlaceId == 7841304988 then
		repeat wait() until game:IsLoaded()
		spawn(function()
			wait(50)
			game:Shutdown()
		end)
		wait(1)
		if workspace:FindFirstChild("egg_from_action_by_user") then
			fireclickdetector(workspace["egg_from_action_by_user"].ClickDetector)
		end
		game:GetService("TeleportService"):Teleport(9372535855, game.Players.LocalPlayer);
	end
	if game.PlaceId == 7772810845 then
		game:GetService("TeleportService"):Teleport(9372535855, game.Players.LocalPlayer);
	end
end
for i,v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
	v:Disable()
end
