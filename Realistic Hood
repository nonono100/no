local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/nonono100/no/main/UI"))()

local function Label(part, objecttext)

	local billboardInstance = Instance.new("BillboardGui")		
	billboardInstance.Parent = part.Parent
	billboardInstance.Name = "Label1"
	billboardInstance.StudsOffset = Vector3.new(0,2,0)
	billboardInstance.Size = UDim2.new(0,200,0,50)
	billboardInstance.AlwaysOnTop = true

	local textLabel = Instance.new("TextLabel", billboardInstance)
	textLabel.BackgroundTransparency = 1
	textLabel.Font = Enum.Font.SourceSansBold
	textLabel.Size = UDim2.new(1,0,0,10)
	if objecttext then
		textLabel.Text = part.ObjectText
	else
		textLabel.Text = part.ActionText
	end
	textLabel.TextSize = 18
	textLabel.RichText = true
	textLabel.TextColor3 = Color3.fromRGB(0,214,0)
	textLabel.TextStrokeColor3 = Color3.fromRGB(0,0,0)
	textLabel.TextStrokeTransparency = 0
	textLabel.TextScaled = true
end
local extools = {}
getgenv().weaponmod = false
getgenv().vehiclemod = false
local function exec(p1)
	local a = require(p1)
	a.MinRecoilPower = 0 
	a.MaxRecoilPower = 0 
	a.gunRecoil = nil 
	a.camRecoil = nil
	a.FireModes.ChangeFiremode = true
	a.FireModes.Auto = true
	a.ShootType = 1
	a.AmmoInGun = math.huge
	a.CrossHair = true 
	a.CenterDot = true
	a.MinSpread = 0 
	a.MaxSpread = 0 
	a.AimInaccuracyStepAmount = 0
	a.BulletDrop = 0
end
local function exec1(p1)
	local a = require(p1)
	a.AutoFlip = true
	a.Horsepower = 1000
	a.Weight = 10000
	a.CGHeight = 0
	a.FAntiRoll = -100
	a.SteerInner = 100 
	a.Config = "AWD"
	a.FBrakeForce = 120000 
	a.RBrakeForce = 120000
end
local function connectBP()
	game.Players.LocalPlayer.Backpack.ChildAdded:Connect(function(child)
		wait(0.1)
		if child:IsA("Tool") and child:FindFirstChild("ACS_Settings") and not extools[child] then
			if getgenv().weaponmod == true then
				extools[child] = true
				exec(child["ACS_Settings"])
			end
		end
	end)
end
connectBP()
workspace.ChildAdded:Connect(function(child)
	wait(1)
	if child:IsA("Model") and child:FindFirstChild("A-Chassis Tune") then
	    print(child.Name)
		if getgenv().vehiclemod == true then
		    print("true")
			exec1(child["A-Chassis Tune"])
		end
	end
end)
game.Players.LocalPlayer.CharacterAdded:Connect(function()
	extools = {}
	connectBP()
	spawn(pcall(function()
		wait(1)
		game:GetService("Players").yamulka.PlayerGui.StatusUI.Efeitos:Destroy()
	end))
end)

