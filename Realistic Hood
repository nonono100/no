local no1 = {
	"$1000";
	"$1100";
	"$800";
	"$500";
	"$600";
	"$900";
	"$250";
	"STEAL";
}
local function weaponesp(value)
	if value == true then
		for i,v in pairs(workspace:GetDescendants()) do 
			if v:IsA("ProximityPrompt") and v.Parent and v.Parent:IsA("Part") and not table.find(no, v.ActionText:lower()) then
				Label(v)
			end
			if v:IsA("ProximityPrompt") and v.Parent and v.Parent:IsA("Part") and table.find(no1, v.ActionText) then
				Label(v, true)
			end
		end
	else
		for i,v in pairs(workspace:GetDescendants()) do 
			if v.Name == "Label1" then
				v:Destroy()
			end
		end
	end
end



local window = {} do
	local windowMeta = {} 
	windowMeta.__index = windowMeta

	function windowMeta:Tab(name)
		local tab = self.window.New({
			Title = name;
		})

		return setmetatable({
			window = self.window;
			tab = tab;
		}, windowMeta) 
	end

	function windowMeta:Toggle(name, callback)
		self.tab.Toggle({
			Text = name;
			Callback = callback;
		})
	end

	function windowMeta:Banner(name, options)
		local _menu = {}

		for name, str in next, options do
			_menu[name] = function() self.window.Banner({ Text = str }) end
		end

		self.tab.Button({
			Text = name;
			Menu = _menu
		})
	end

	function window.new(name)
		local main = Material.Load({
			Title = name;
			Style = 1;

			Theme = 'Dark';
			SizeX = 300;
			SizeY = 320;
		})

		return setmetatable({ window = main }, windowMeta) 
	end
end

local gui = window.new("Realistic Hood")
local tab = gui:Tab("Main")

tab:Toggle("Weapon Mod", function(value) 
	getgenv().weaponmod = value
	repeat wait() until game.Players.LocalPlayer and game.Players.LocalPlayer.Character
	if value == true then
		for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
			if v:IsA("Tool") and v:FindFirstChild("ACS_Settings") then
				exec(v["ACS_Settings"])
			end
		end
		for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do 
			if v:IsA("Tool") and v:FindFirstChild("ACS_Settings") then
				exec(v["ACS_Settings"])
			end
		end
	end

end) 
tab:Toggle("Vehicle Mod", function(value) 
	getgenv().vehiclemod = value 
	if value == true then
		for i,v in pairs(workspace:GetDescendants()) do 
			if v:IsA("ModuleScript") and (string.find(v.Name:lower(), "chassis") or string.find(v.Name:lower(), "tune")) then
				exec1(v)
			end
		end
	end
end)
tab:Toggle("Weapon ESP", function(value) 
	weaponesp(value)
end)
